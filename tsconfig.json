{
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "rootDir": "src",
    "resolveJsonModule": true,
    "removeComments": true,
    "outDir": "./dist",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "incremental": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "module": "ESNext", // Updated
    "target": "ESNext", // Updated
    "types": ["node"],
    "lib": ["esnext"], // Update
  },
  "include": ["src"],
  "exclude": ["node_modules", "./test", "./dist", "./prisma"],
}
